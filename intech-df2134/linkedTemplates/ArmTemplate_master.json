{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "intech-df2134"
		},
		"AzureDataLakeStoragelinkservices_accountKey": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « accountKey » de « AzureDataLakeStoragelinkservices »"
		},
		"AzureDataLakeStoragelinkservices_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://intechrg.dfs.core.windows.net/"
		},
		"AzureDatabrickslinkedservices_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0218-102809-t1p6j2lz"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://intech-kv2134.vault.azure.net/"
		},
		"SqlServerOnprimelinkedservices_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "WWW\\SQLEXPRESS"
		},
		"SqlServerOnprimelinkedservices_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2022"
		},
		"SqlServerOnprimelinkedservices_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "luke"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "intech-df2134_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStoragelinkservices_accountKey": {
						"value": "[parameters('AzureDataLakeStoragelinkservices_accountKey')]"
					},
					"AzureDataLakeStoragelinkservices_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragelinkservices_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabrickslinkedservices_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabrickslinkedservices_properties_typeProperties_existingClusterId')]"
					},
					"SqlServerOnprimelinkedservices_properties_typeProperties_server": {
						"value": "[parameters('SqlServerOnprimelinkedservices_properties_typeProperties_server')]"
					},
					"SqlServerOnprimelinkedservices_properties_typeProperties_database": {
						"value": "[parameters('SqlServerOnprimelinkedservices_properties_typeProperties_database')]"
					},
					"SqlServerOnprimelinkedservices_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerOnprimelinkedservices_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}